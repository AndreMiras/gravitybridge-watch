{
  "name": "gravitybridge-watch",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:debug": "NODE_OPTIONS='--inspect' next dev",
    "build:wrangler": "wrangler deploy --dry-run --outdir=build",
    "build": "next build",
    "start": "next start",
    "lint:prettier": "prettier --check src .github docker *.{js,ts} *.md",
    "format:prettier": "prettier --write src .github docker *.{js,ts} *.md",
    "lint": "eslint . && npm run lint:prettier",
    "format": "npm run format:prettier",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "engines": {
    "node": ">=24.0.0",
    "npm": ">=10.0.0"
  },
  "dependencies": {
    "@google-cloud/storage": "^7.7.0",
    "@grpc/grpc-js": "^1.9.15",
    "@protobufs/osmosis": "^0.4.0",
    "next": "^16.0.10",
    "prom-client": "^15.1.0",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241112.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.15",
    "autoprefixer": "^10.0.1",
    "eslint": "^9.39.2",
    "eslint-config-next": "^16.0.10",
    "jsdom": "^27.2.0",
    "postcss": "^8",
    "prettier": "^3.1.1",
    "tailwindcss": "^3.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.15",
    "wrangler": "^4.53.0"
  }
}
